<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="532.0" prefWidth="984.0" styleClass="container" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionaryGUI.addController">
   <children>
      <Label fx:id="ok" layoutX="800.0" layoutY="417.0" text="added successfully">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="addBtn" layoutX="830.0" layoutY="466.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="96.0" text="add">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextArea fx:id="wordExplain" layoutX="29.0" layoutY="197.0" prefHeight="310.0" prefWidth="735.0" promptText="type meaning..." styleClass="explanation-field" stylesheets="@style.css" />
      <Label fx:id="existed" layoutX="621.0" layoutY="90.0" text="this word is already existed" />
      <Label layoutX="30.0" layoutY="37.0" prefHeight="70.0" prefWidth="245.0" text="add word" textFill="#9a3e72">
         <font>
            <Font name="Baloo 2 ExtraBold" size="30.0" />
         </font>
      </Label>
      <Pane layoutX="30.0" layoutY="118.0" prefHeight="70.0" prefWidth="735.0" styleClass="word-bar" stylesheets="@style.css">
         <children>
            <TextField fx:id="wordTarget" layoutY="1.0" prefHeight="70.0" prefWidth="735.0" promptText="type word..." style="-fx-background-color: transparent;" />
         </children>
      </Pane>
   </children>
</AnchorPane>
